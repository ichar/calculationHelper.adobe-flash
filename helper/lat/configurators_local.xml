<!DOCTYPE document
	[
		<!ELEMENT document (system, task, description, lanquage, configurator+)>
			<!ATTLIST	document
						id		ID		#REQUIRED
						type	NMTOKEN	#REQUIRED
			>
			<!ELEMENT system EMPTY>
				<!ATTLIST	system
							id		ID		#REQUIRED
							version	NMTOKEN	#REQUIRED
				>
			<!ELEMENT task EMPTY>
				<!ATTLIST	task
							id		ID		#REQUIRED
							version	NMTOKEN	#REQUIRED
				>
			<!ELEMENT description EMPTY>
				<!ATTLIST	description
							id		ID		#REQUIRED
							version	NMTOKEN	#REQUIRED
				>
			<!ELEMENT lanquage EMPTY>
				<!ATTLIST	lanquage
								id		ID		#REQUIRED
				>
			<!ELEMENT configurator (field+)>
				<!ATTLIST	configurator
							id		ID		#REQUIRED
				>
				<!ELEMENT field ANY>
						<!ATTLIST	field
									name	NMTOKEN	#REQUIRED
						>
	]
>

